S=["ANU","RAJ","SAM","ANI","BALU","ROSE","BEN","TOM","ROY"]
M=[[22,33,98],[63,90,21],[76,11,20],[43,62,94],[76,49,56],[76,34,84],[76,71,47],[44,57,83],[44,83,44],[11,22,33]]
A=[False,False,True,False,False,False,True,False,False,True]
per=[sum(i)//3 for i in M]
for i in range(len(S)):
    if(per[i]>=40 and A[i]==False):
        print(i+1,".",per[i],"% No Arrear")
